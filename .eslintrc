{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": 0,
    "no-underscore-dangle": 0,
    "indent": [2, 2],
    "semi": [2, "always"],
    "quotes": [2, "single"]
  },
  "plugins": [
    "react"
  ]
}
